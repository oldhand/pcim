<?xml version="1.0" encoding="UTF-8"?>
<Window>
    <ListContainerElement class="listitem" height="50">
        <HBox padding="5,0,10,0" childmargin="5" mouse="false">
            <Box width="40" height="40" valign="center" mouse="false" mousechild="false">
                <Control name="merge_item_icon" width="38" height="38" borderround="38,38,38,38" margin="1,1,0,0"/>
                <Control class="icon_headimage_mask_40x40"/>
            </Box>
            <Label name="merge_item_name" width="stretch" valign="center" font="1" mouse="false"/>

            <Box width="auto" height="auto" float="true" halign="right" valign="center">
                <Label name="merge_item_unread" width="auto" minwidth="18" height="18" textpadding="2,1,2,2" align="center" halign="center" bkimage="file='../public/icon/icon_number.png' corner='8,7,8,7'" normaltextcolor="white" mouse="false" visible="false"/>
                <Button class="btn_del_search" name="merge_item_close" width="16" height="16" halign="center" valign="center" visible="false"/>
            </Box>             
        </HBox>
        <Control class="splitline_hor_level1" float="true" valign="bottom" mouse="false"/>
    </ListContainerElement>
</Window>
